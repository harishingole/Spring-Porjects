<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
		"http://www.springframework.org/dtd/spring-beans-2.0.dtd">
		
<beans>
	<bean id="t" class="beans.Test">
		<property name="fruits">
			<list>
				<value>Apple</value>
				<value>Mango</value>
				<value>Grapes</value>
			</list>
		</property>
		<property name="crickators">
			<set>
				<value>M. S. Dhoni</value>
				<value>S. Ganguly</value>
				<value>R. Dravid</value>
			</set>
		</property>
		<property name="countriesAndCapitals">
			<map>
				<entry key="India" value="Pune" />
				<entry key="Pakistan" value="Lahor" />
			</map>
		</property>
	</bean>
</beans>